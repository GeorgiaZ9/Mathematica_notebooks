Cell[CellGroupData[{

Cell["xTras loading and Manifold Definition", "Subsection",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
 CellChangeTimes->{{3.9527730516761*^9, 3.9527730836618347`*^9}, 
   3.952773376689516*^9},ExpressionUUID->"c5ac4c53-8f39-1646-b6d3-\
4bb529cbf000"],

Cell["\<\
A 4-dimensional manifold is defined and it is described by the corresponding \
Greek indices. With the metric definition, the covariant derivative is \
included along with the Christoffel symbols and curvature tensors such as the \
Riemann tensor, Ricci tensor and scalar. Finally, the flat Minkowski metric \
should be included along with the partial derivative.\
\>", "Text",
 CellGroupingRules->{"GroupTogetherGrouping", 10000.},
 CellChangeTimes->{{3.9527730960133667`*^9, 
  3.952773376689516*^9}},ExpressionUUID->"6cc175bb-5f61-414a-9837-\
54871266c788"],

Cell[BoxData[{
 RowBox[{"Needs", "[", "\"\<xAct`xTras`\>\"", "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"DefManifold", "[", 
   RowBox[{"M4", ",", "4", ",", 
    RowBox[{"{", 
     RowBox[{
     "\[Alpha]", ",", "\[Beta]", ",", "\[Gamma]", ",", "\[Delta]", ",", 
      "\[Mu]", ",", "\[Nu]", ",", "\[Rho]", ",", "\[Sigma]"}], "}"}]}], "]"}],
   ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"DefMetric", "[", 
    RowBox[{
     RowBox[{"-", "1"}], ",", 
     RowBox[{"metric", "[", 
      RowBox[{
       RowBox[{"-", "\[Alpha]"}], ",", 
       RowBox[{"-", "\[Beta]"}]}], "]"}], ",", "CD", ",", 
     RowBox[{"CurvatureRelations", "->", "True"}], ",", 
     RowBox[{"PrintAs", "->", "\"\<g\>\""}]}], "]"}], ";"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", " ", 
   RowBox[{"Minkowski", " ", "Metric", " ", "definition"}], " ", 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{"DefMetric", "[", 
  RowBox[{
   RowBox[{"{", 
    RowBox[{"3", ",", "1", ",", "0"}], "}"}], ",", 
   RowBox[{"Mink", "[", 
    RowBox[{
     RowBox[{"-", "\[Mu]"}], ",", 
     RowBox[{"-", "\[Nu]"}]}], "]"}], ",", "PD", ",", 
   RowBox[{"{", 
    RowBox[{"\"\<,\>\"", ",", "\"\<\[PartialD]\>\""}], "}"}], ",", 
   RowBox[{"FlatMetric", "\[Rule]", "True"}], ",", 
   RowBox[{"PrintAs", "\[Rule]", "\"\<\[Eta]\>\""}]}], "]"}]}], "Input",
